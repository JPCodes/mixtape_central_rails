<%= render 'layouts/errors', :object => @mixtape %>

<%= form_for [@user, @mixtape] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <%= f.label :subgenre %>
  <%= f.text_field :subgenre %>
  <%= f.label :year %>
  <%= f.number_field :year %>
  <%= f.submit %>
<% end %>
